{
  "name": "strategic-projects-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev --hostname 127.0.0.1 --port 3000",
    "build": "next build",
    "start": "next start",
    "clean": "rm -rf .next tsconfig.tsbuildinfo",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "db:seed": "node prisma/seed.cjs",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:seed:staging": "node --experimental-strip-types prisma/seed.ts",
    "staging:credentials": "node scripts/generate-staging-credentials.mjs"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "next": "^16.1.6",
    "next-auth": "^4.24.11",
    "react": "^19.0.1",
    "react-dom": "^19.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.4.49",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
